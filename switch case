public class SwitchCase {
public static void main(String args[]){
      int i=2;
      switch(i)
      {
	 case 1:
	   System.out.println("hello ");
	   break;
	 case 2:
	   System.out.println("hii ");
	   break;
	 case 3:
	   System.out.println("bonjour ");
	   break;
	 case 4:
           System.out.println("welcome ");
           break;
	 default:
	   System.out.println("Default ");
      }
   }
}
